	<script type="text/javascript">
		$(document).on("click",".quickview",function(e){
			var modalString = "/ajax/modal.cfm";
			var partNumber = $(this).data('partnumber');
			$.ajax({
				type: "get",
				url: modalString,
				data: {custTagname:'quickview',myStruc:partNumber},
				success: function(resp) {
					if($("#qv"+partNumber.replace(/\//,"_")).length == 0){
						$('#modalplaceholder').append(resp);
					}
					$("#qv"+partNumber.replace(/\//,"_")).dialog({
						position: [e.clientX,e.clientY],
						minWidth: 450,
						resizable: false
					});
				}
			});
		});
	</script>

