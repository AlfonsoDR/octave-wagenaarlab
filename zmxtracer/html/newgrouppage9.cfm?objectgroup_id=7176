	  		  		$('#hotsp').width(($(this).width())+ 200);
	  		 		$('#hotsp').addClass('hotspclass');
				 if($(this).width() < 800) {
				 	 $('#hotsp').css( "margin-left", "25px" );
				 	 $('#hotsp').css( "margin-right", "25px" );
		 		 }
		})
	})
	});
</script>
